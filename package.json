{
  "name": "@redilink/source",
  "version": "1.0.0",
  "workspaces": [
    "packages/*",
    "Context/*",
    "apps/*"
  ],
  "scripts": {
    "build": "NODE_ENV=production npx nx run-many -t build && cp -r apps/frontend/dist apps/backend/dist/public",
    "start": "npx nx start @redilink/backend",
    "dev:backend": "npx nx dev @redilink/backend",
    "dev:frontend": "npx nx dev @redilink/frontend",
    "dev": "NODE_ENV=dev npx nx run-many -t dev"
  },
  "dependencies": {
    "@astrojs/tailwind": "^5.1.0",
    "@prisma/client": "^5.14.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-module-utils": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-scope": "^7.2.2",
    "eslint-visitor-keys": "^3.4.3",
    "nx": "19.1.0",
    "prettier": "^2.8.8"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
