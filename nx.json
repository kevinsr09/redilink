{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "test": {
      "dependsOn": [
        "^test"
      ]
    },
    "dev":{
      "cache": false,
      "dependsOn": [
        "dev"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ],
      "cache": true
    },
    "start": {
      "dependsOn": [
        "start"
      ]
    }
  },
  "defaultBase": "main"
}